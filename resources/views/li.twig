<li class="{{ container.class }}">
	<a href="#" class="{{ toggle.class }}" data-toggle="dropdown">{{ toggle.title|raw }}</a>
	<ul class="{{ ul.class }}">
	  	{% for locale in locales %}
	  		<li class="{{ li.class }}">
				<a class="{{ a.class }}" href="{{ locale.url }}">{{ locale.name }}</a>
			</li>
	  	{% endfor %}
	</ul>
</li>